{
  "name": "fcp-cv-frontend",
  "version": "0.0.0",
  "description": "CDP Frontend Template",
  "type": "module",
  "engines": {
    "node": "24"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint . && prettier --check .",
    "start": "node server.js",
    "test": "vitest run --coverage",
    "test:accessibility": "axe ${TEST_ENVIRONMENT_ROOT_URL:-http://localhost:3001}",
    "test:accessibility:docker": "docker compose -f compose.yml -f compose.accessibility.yaml -p fcp-cv-frontend-accessibility-test run --quiet-pull --pull always --build --rm accessibility",
    "posttest:accessibility:docker": "docker compose -p fcp-cv-frontend-accessibility-test down -v --remove-orphans"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@azure/msal-browser": "4.28.1",
    "@azure/msal-react": "3.0.25",
    "@elastic/ecs-pino-format": "1.5.0",
    "@headlessui/react": "2.2.9",
    "@tanstack/react-table": "8.21.3",
    "convict": "6.2.4",
    "jose": "6.1.3",
    "next": "16.1.6",
    "pino": "10.3.0",
    "pino-http": "11.0.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-toastify": "11.0.5",
    "swr": "2.3.8"
  },
  "devDependencies": {
    "@axe-core/cli": "4.11.0",
    "@tailwindcss/postcss": "4.1.18",
    "@vitest/coverage-v8": "4.0.18",
    "clsx": "2.1.1",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.6",
    "husky": "9.1.7",
    "prettier": "3.8.1",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "vitest": "4.0.18"
  }
}
